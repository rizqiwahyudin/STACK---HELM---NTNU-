ta12_2:var2*exp1+var3*exp2;
ta12_3:var2*(-exp2)+var3*(-exp1);
ta12_4:var2*(-exp1)+var3*(-exp2);

ta13_2:-(rand1*rand2+1)/rand1;
ta13_1:-rand3*rand4/rand2;

ta14_1:expA;
ta14_2:expB;

ta14_1_1:expB;
ta14_2_1:expA;

ta15_1:expA/exp1+expB/exp2;
ta15_2:(-expA)/(-exp1)-(expB)/(-exp2);


factor1: factor1;
factor2:factor2;
textExp:factor1*factor2;</text>
