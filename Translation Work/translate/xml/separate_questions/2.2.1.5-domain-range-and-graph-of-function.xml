        <questionvariables>
            <text><![CDATA[a: rand(5)+2
b:rand_with_prohib(2,6,[a])
f: expand(a*x^2+b)
f1: fullratsimp(f)
f(x):=a*x^2 +b
/* domain of function*/
leftend:-3;
rightend:3;
ta6: -3<= x nounand x<= 3;

fatneg3:ev(f,x=-3);
fatneg2: ev(f,x=-2);
ta2: ev(f,x=-1);
ta3: ev(f,x=0);
ta4: ev(f,x=1);
ta5: ev(f,x=2);
fat3: ev(f,x=3);

/* range */
ta7: ta3<=y nounand y<=fat3;

max: f(a/2);
min: f(a/3);

c_funcs: [f];
w_funcs: [-f, f-max, -f+max];
/* Choose functions to plot */
funcs: append(c_funcs, rand_selection(w_funcs,3)) 
/* permute the graphs but keep note of which ones are right! */
reordering:random_permutation([1,2,3,4]);
fntoletter:zip_with("[",reordering,[A,B,C,D]);
/* apply the reordering to produce the shuffled list of options */
funcstoplot: makelist(funcs[reordering[i]],i,1,4);
corr:maplist(lambda([ex],assoc(ex,fntoletter)), [1,2,3,4]);
ta1:maplist(lambda([ex],[ex, is(ex=assoc(1,fntoletter))]), [A,B,C,D]);
thumbnailplot:lambda([ex1,ex2],plot(ex1,[x,-5,5],[y,-30, 30],[box, false],[yx_ratio, 0.7],[xtics, -5, 1, 5],[ytics, -30, 15, 30],[axes, solid],[size,240,240],[plottags,false],[alt,ex2]));]]></text>
        </questionvariables>
