        <questionvariables>
            <text>f1(x):=x;
g1(x):=1/(cos(x));
df1: diff(f1(x),x);
dg1: diff(g1(x),x);
ta1: diff(f1(x)*g1(x),x);


f2(x):=e^(x);
g2(x):=sin(x);
df2: diff(f2(x),x);
dg2: diff(g2(x),x);
ta2: factor(diff(f2(x)*g2(x),x));

f3(x):=x/cos(x);
g3(x):=e^(x)*sin(x);
df3: diff(f3(x),x);
dg3: diff(g3(x),x);
ta3:diff(f3(x)*g3(x),x);
ta3: factor(trigreduce(ta3));


</text>
