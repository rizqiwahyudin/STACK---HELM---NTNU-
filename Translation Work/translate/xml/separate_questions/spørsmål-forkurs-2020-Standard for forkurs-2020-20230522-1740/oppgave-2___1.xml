        <questionvariables>
            <text>exp:2*x^3-8*x;

losn:solve(exp=0,x);
ta1:setify(maplist(rhs,losn));

f1:subst(x=1,exp);
deriv:diff(exp,x);
fd1:subst(x=1,deriv);
likning:y-f1=fd1*(x-1);
likning:solve(likning,y);
ta2:ev(y,likning[1]);
dderiv:diff(deriv,x);
ovre_grense:ev(x,losn[2]);
areal:abs(int(exp,x,0,ovre_grense));
</text>
