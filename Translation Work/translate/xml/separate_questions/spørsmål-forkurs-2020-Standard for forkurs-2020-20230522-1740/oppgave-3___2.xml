        <questionvariables>
            <text>teller:(2*x^2);
nevner:(x^2-4);
exp:teller/nevner;

nevner_null:solve(nevner=0,x);
udef:setify(maplist(rhs,nevner_null));
losn:solve(exp=0,x);
nullpkt:setify(maplist(rhs,losn));
vertikal:{2};
deriv:diff(exp,x);
topp_x:solve(deriv=0,x);
topp_x:ev(x,topp_x[1]);
topp_y:solve(deriv=topp_x,x);
topp_y:ev(x,topp_y[1]);
dderiv:diff(deriv,x);


</text>
