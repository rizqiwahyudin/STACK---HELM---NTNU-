        <questionvariables>
            <text><![CDATA[stackvectortex(ex):= block(sconcat("{ \\underline{", tex1(first(args(ex))), "}}"));
ordergreat(i,j,k);
 /*Sets up relevant vectors to be used in the question.*/
 p1: matrix([9],[9],[8]) - rand(matrix([18],[18],[7]));
drRowVec: transpose(p1);
 u1: matrix([i],[j],[k]);
 /*Presents the vectors p in a suitable form to be displayed in the question text / note.*/ 
 P1: texboldatoms(dotproduct(p1,u1)); 
 /*Computes the vectors' respective magnitudes.*/
 mag_p1: sqrt(dotproduct(p1,p1));
 /*Evaluates cos theta in terms of the scalar product and the magnitudes.*/
 cos_thetax: p1[1,1]/(mag_p1);
 cos_thetay: p1[2,1]/(mag_p1);
 cos_thetaz: p1[3,1]/(mag_p1);
 /*Finds the value of theta (truncated to three sig fig) in both radians and degrees.*/
 r_thetax_r: significantfigures(ev(acos(cos_thetax),float),3);
 r_thetax_d: significantfigures(ev(acos(cos_thetax)*(180/pi),float),3);
 r_thetay_r: significantfigures(ev(acos(cos_thetay),float),3);
 r_thetay_d: significantfigures(ev(acos(cos_thetay)*(180/pi),float),3);
 r_thetaz_r: significantfigures(ev(acos(cos_thetaz),float),3);
 r_thetaz_d: significantfigures(ev(acos(cos_thetaz)*(180/pi),float),3);
 /*Students may possibly sum the components of the vector product as one of their answers. This item may be used as part of the PRT, to give relevant feedback about their error.*/
 /*q1 is basis vector for x axis, should probably include y and z*/
q1: matrix([1],[0],[0]);
 vector_sum:(p1[2,1]*q1[3,1] - p[3,1]*q1[2,1]) + (p1[3,1]*q1[1,1] - p1[1,1]*q1[3,1]) + (p1[1,1]*q1[2,1] - p1[2,1]*q1[1,1]);
 /*entri that wont occur for test of ECF*/
ecfTest: matrix([10,20,30]);
ecfTestCosx: 1/sqrt(14);
ecfTestCosy: 2/sqrt(14);
ecfTestCosz: 3/sqrt(14);
 r_ecfthetax_d: significantfigures(ev(acos(ecfTestCosx)*(180/pi),float),3);
 r_ecfthetay_d: significantfigures(ev(acos(ecfTestCosy)*(180/pi),float),3);
 r_ecfthetaz_d: significantfigures(ev(acos(ecfTestCosz)*(180/pi),float),3);]]></text>
